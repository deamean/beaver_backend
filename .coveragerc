[run]
# 覆盖率运行配置
source = biz
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */migrations/*
    */__init__.py

[report]
# 报告配置
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
